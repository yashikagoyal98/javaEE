class Icecream
 {
	 private String flavour;
	 private float price;
	 private float discount;

	/*
	 * public String getFlavour() { return flavour; }
	 * 
	 * public void setFlavour(String flavour) { this.flavour = flavour; }
	 * 
	 * public float getPrice() { return price; }
